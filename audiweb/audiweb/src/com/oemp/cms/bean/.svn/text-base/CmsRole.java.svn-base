package com.oemp.cms.bean;

import java.util.Date;


public class CmsRole {
    private String cmroName ;           //角色名称 是主键
    private String cmroDesc;            //角色描述
    private Date cmroAddtime;			//添加时间
    public Date getCmroAddtime() {
		return cmroAddtime;
	}
	public void setCmroAddtime(Date cmroAddtime) {
		this.cmroAddtime = cmroAddtime;
	}
	private String cmroAdduser;			//添加人
	public String getCmroName() {
		return cmroName;
	}
	public void setCmroName(String cmroName) {
		this.cmroName = cmroName;
	}
	public String getCmroDesc() {
		return cmroDesc;
	}
	public void setCmroDesc(String cmroDesc) {
		this.cmroDesc = cmroDesc;
	}

	public String getCmroAdduser() {
		return cmroAdduser;
	}
	public void setCmroAdduser(String cmroAdduser) {
		this.cmroAdduser = cmroAdduser;
	}
    
}
